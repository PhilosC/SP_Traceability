    }
  ],
  "paths": {
    "/SPTracePara_OBSet": {
      "get": {
        "summary": "get OB information",
        "tags": [],
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ExOBQty": {
                      "$ref": "../models/OB_QTY.v1.json"
                    },
                    "ExOBDetail": {
                      "$ref": "../models/OB_Detail.v1.json"
                    },
                    "ExMsg": {
                      "type": "object",
                      "properties": {
                        "Delivery": {
                          "type": "string",
                          "minLength": 10,
                          "maxLength": 10,
                          "example": "36010777"
                        },
                        "Item": {
                          "type": "string",
                          "minLength": 6,
                          "maxLength": 6
                        },
                        "Type": {
                          "type": "string",
                          "minLength": 1,
                          "maxLength": 1,
                          "example": "\"E\""
                        },
                        "Message": {
                          "type": "string",
                          "maxLength": 220,
                          "example": "\"No traceability material in this outbound, No scan necessary!\""
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "operationId": "get-get_ob",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "minLength": 10,
              "maxLength": 10
            },
            "in": "query",
            "name": "Delivery",
            "required": true,
            "description": "Outbound delivery number"
          }
        ],
        "description": "This API is used for getting OB information(OB to be scanned, results are OB box QTY , this is the 1st step in SP Traceability process)."
      },
      "parameters": []
    }
  },
  "components": {
    "schemas": {},
    "securitySchemes": {
      "API Key - Test": {
        "type": "apiKey",
        "in": "query"
      },
      "API Key - Production": {
        "type": "apiKey",
        "in": "query"
      }
    }
  }
}